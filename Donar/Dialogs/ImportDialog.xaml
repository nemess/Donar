<Window x:Class="Donar.Dialogs.ImportDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Donar.Dialogs"
        mc:Ignorable="d"
        Title="Select Import Plugins!" Height="131.4" Width="423.2">
    <Grid>
        <TextBlock x:Name="PluginTitle" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Import Plugin:" VerticalAlignment="Top"/>
        <ComboBox x:Name="Plugins" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="300"/>
        <Button x:Name="OK" Content="Import" HorizontalAlignment="Left" Margin="210,60,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="OK_Click"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="315,60,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>

    </Grid>
</Window>
